{
  "name": "<%= appName %>",
  "version": "0.0.1",
  "scripts": {
    "start": "./app/node_modules/electron/dist/electron ./app/index.js prod",
    "start:dev": "./app/node_modules/electron/dist/electron ./app/index.js dev",
    "start:prod": "./app/node_modules/electron/dist/electron ./app/index.js prod",
    "build:dev": "webpack-dev-server --progress --colors --env.type=dev",
    "build:prod": "webpack --config ./webpack.config.js --progress --colors --profile --bail --env.type=prod",
    "tslint": "tslint -c ./tslint.json 'dev/**/*.ts'; exit 0;"
  },
  "dependencies": {
    "@angular/animations": "^5.1.3",
    "@angular/common": "^5.1.3",
    "@angular/compiler": "^5.1.3",
    "@angular/core": "^5.1.3",
    "@angular/forms": "^5.1.3",
    "@angular/http": "^5.1.3",
    "@angular/platform-browser": "^5.1.3",
    "@angular/platform-browser-dynamic": "^5.1.3",
    "@angular/router": "^5.1.3",<% if(materialDesign) { %>
    "@angular/material": "^5.0.3",
    "@angular/cdk": "^5.0.3",
    "material-design-icons": "^3.0.1",<% } %><% if(hmr) { %>
    "@angularclass/hmr": "^2.1.3",<% } %>
    "core-js": "^2.5.3",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.44",
    "@types/node": "^9.3.0",
    "codelyzer": "^4.0.2",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "loader-utils": "^1.1.0",
    "html-webpack-plugin": "^2.30.1",<% if(templateEngine) { %>
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",<% } else { %>
    "html-loader": "^0.5.4",<% } %>
    "style-loader": "^0.19.1",<% if(cssPreprocessor == 'Stylus') { %>
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",<% } %><% if(cssPreprocessor == 'Less') { %>
    "less": "^2.7.3",
    "less-loader": "^4.0.5",<% } %><% if(cssPreprocessor == 'Sass') { %>
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",<% } %>
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0"
  },
  "private": true
}
